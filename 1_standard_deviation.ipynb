    "def solution(N, X):\n",
    "    mean = sum(X)/N\n",
    "    return (sum([(mean-x)**2 for x in X])/N)**(1/2)\n",
    "    \n",
    "N = int(input())\n",
    "X = [int(x) for x in input().split(\" \")]\n",
    "print ('%.1f'%solution(N, X))"
   ]
